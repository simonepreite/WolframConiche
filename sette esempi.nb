ToExpression["ESEMPI", TraditionalForm]

ToExpression["UNO", TraditionalForm]
Print[HoldForm[ 4*x^2 - 8*x + 4*y^2 + 2*y + 1] // 
  TraditionalForm, "=", 0 // TraditionalForm]
Print[HoldForm[4*(x^2 - 2 x + 1) + 4*(y^2 + y/2 + 1/16)], "=", -1 + 
  4 + 1/4]
Print[HoldForm[4 (x - 1)^2 + 4 (y + 1/4)^2], "=", -1 + 4 + 1/4]
Print[HoldForm[(x - 1)^2 + (y + 1/4)^2], "=", 13/16]
TraditionalForm["la conice e una circonferenza di centro (1,-1/4) e \
raggio r \[Sqrt]13/4"]


ToExpression["DUE", TraditionalForm]
Print[HoldForm[ -4*x^2 + 8*x - 2*y^2 + 12] // TraditionalForm, "=", 
 0 // TraditionalForm]
Print[HoldForm[-4*(x^2 - 2 x + 1) - 2 y^2], "=", -16]
Print[HoldForm[-4 (x - 1)^2 - 2 y^2], "=", -16]
Print[HoldForm[(x - 1)^2/4 + y^2/8], "=", 1]
Print[HoldForm[a], "=", 2]
Print[HoldForm[b], "=", Sqrt[8], "=", 2 Sqrt[2]]
TraditionalForm["VERTICI (-1,0) (3,0) (1,-2\[Sqrt]2) (1,2\[Sqrt]2)"]
TraditionalForm["FUOCHI(1,-2) (1,2)"]

ToExpression["TRE", TraditionalForm]
Print[HoldForm[ 4*x^2 + 8*x - 2*y^2 + 12] // TraditionalForm, "=", 
 0 // TraditionalForm]
Print[HoldForm[4*(x^2 + 2 x + 1) - 2 y^2], "=", -8]
Print[HoldForm[4 (x + 1)^2 - 2 y^2], "=", -8]
Print[HoldForm[(x + 1)^2/2 - y^2/4], "=", -1]
TraditionalForm["iperbole con i fuochi sull'asse \
\[LeftDoubleBracketingBar] Oy ,O(-1,0)"]
Print[HoldForm[a], "=", Sqrt[2]]
Print[HoldForm[b], "=", 2]
TraditionalForm["VERTICI (-1-\[Sqrt]2,0)  (-1+\[Sqrt]2,0)  (-1,-2)  \
(-1,2)"]
TraditionalForm["FUOCHI(1,-\[Sqrt]6) (-1,\[Sqrt]6)"]

ToExpression["QUATRO", TraditionalForm]
Print[HoldForm[2*y^2 - 4*x + y - 1] // TraditionalForm, "=", 
 0 // TraditionalForm]
Print[HoldForm[x], "=", 1/2*y^2 + 1/4*y - 1/4]
Print[HoldForm[a], "=", 1/2]
Print[HoldForm[b], "=", 1/4]
Print[HoldForm[c], "=", -1/4]
Print[HoldForm[b^2 - 4 a*c], "=", 9/16]
TraditionalForm["ASSE y=-b/2a y=-1/4"]
TraditionalForm["FUOCO((1-\[DifferenceDelta])/4a,-b/2a)=(7/32,-1/4)"]
TraditionalForm["VERTICE (-\[DifferenceDelta]/4a,-b/2a)=(-9/32,-1/4)"]
TraditionalForm["DIRETTRICE x=-(1+\[DifferenceDelta])/4a=-25/32"]


ToExpression["CINQUE", TraditionalForm]
Print[HoldForm[x^2 - 2*x - y - 3] // TraditionalForm, "=", 
 0 // TraditionalForm]
Print[HoldForm[y], "=", x^2 - 2*x - 3]
Print[HoldForm[a], "=", 1]
Print[HoldForm[b], "=", -2]
Print[HoldForm[c], "=", -3]
Print[HoldForm[b^2 - 4 a*c], "=", 16]
TraditionalForm["ASSE y=-b/2a x=-1"]
TraditionalForm["FUOCO(-b/2a,(1-\[DifferenceDelta])/4a)=(-1,-15/4)"]
TraditionalForm["VERTICE(-b/2a,-\[DifferenceDelta]/4a)=(-1,-4)"]
TraditionalForm["DIRETTRICE x=-(1+\[DifferenceDelta])/4a=-17/4"]


ToExpression["SEI", TraditionalForm]
Print[HoldForm[ 4*x^2 - y^2 - 6 x - 5 y - 4] // TraditionalForm, "=", 
 0 // TraditionalForm]
Print[HoldForm[ 4*x^2 - 6 x - y^2 - 5 y - 4] // TraditionalForm, "=", 
 0 // TraditionalForm]
Print[HoldForm[4*(x^2 - 3/2*x + 9/16) - (y^2 + 5*y + 25/4)], "=", 
 4 + 9/4 - 25/4]
Print[HoldForm[4 (x - 3/4)^2 - (y + 5/2)^2], "=", 0]
Print[HoldForm[(2 x - 3/2)^2 - (y + 5/2)^2], "=", 0]
Print[HoldForm[(2 x - 3/2 + y + 5/2) (2 x - 3/2 - y - 5/2)], "=", 0]
TraditionalForm["(2x+y+1)(2x--y-4)=0 due rette"]

